{
  "name": "angular-gulp-boilerplate",
  "version": "0.6.0",
  "description": "Clean but full featured AngularJS boilerplate using Gulp workflow and best practicies",
  "bin": "./cli.js",
  "scripts": {
    "docs:gulp": "gulpdoc ./gulp/*.js",
    "lint": "eslint ./cli.js ./gulpfile.js ./gulp/ ./src/ ./test/",
    "update:bower": "ncu -a -m bower --packageFile bower.json",
    "update:dev": "ncu -a --packageFile package.json"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/1oginov/Angular-Gulp-Boilerplate.git"
  },
  "keywords": [
    "angular",
    "angularjs",
    "boilerplate",
    "concatenation",
    "gulp",
    "i18n",
    "internationalization",
    "l10n",
    "localization",
    "minimisation",
    "seed",
    "skeleton",
    "uglification"
  ],
  "author": "Danila Loginov <danila@loginov.rocks> (https://loginov.rocks)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/1oginov/Angular-Gulp-Boilerplate/issues"
  },
  "homepage": "https://github.com/1oginov/Angular-Gulp-Boilerplate",
  "devDependencies": {
    "babel-core": "^6.26.0",
    "babel-preset-env": "^1.6.1",
    "browser-sync": "^2.23.6",
    "del": "^3.0.0",
    "eslint": "^4.18.2",
    "eslint-config-angular": "^0.5.0",
    "eslint-plugin-angular": "^3.3.0",
    "git-clone": "^0.1.0",
    "gulp": "^3.9.1",
    "gulp-angular-filesort": "^1.1.1",
    "gulp-angular-templatecache": "^2.2.0",
    "gulp-autoprefixer": "^5.0.0",
    "gulp-babel": "^7.0.1",
    "gulp-cssnano": "^2.1.2",
    "gulp-debug": "^3.2.0",
    "gulp-eslint": "^4.0.2",
    "gulp-filter": "^5.1.0",
    "gulp-flatten": "^0.4.0",
    "gulp-htmlmin": "^4.0.0",
    "gulp-inject": "^4.3.1",
    "gulp-load-plugins": "^1.5.0",
    "gulp-locales-bundler": "^1.0.0",
    "gulp-ng-annotate": "^2.1.0",
    "gulp-rev": "^8.1.1",
    "gulp-rev-replace": "^0.4.4",
    "gulp-sass": "^4.0.0",
    "gulp-size": "^3.0.0",
    "gulp-sourcemaps": "^2.6.4",
    "gulp-uglify": "^3.0.0",
    "gulp-useref": "^3.1.4",
    "gulp-util": "^3.0.8",
    "gulpdoc": "^1.1.1",
    "main-bower-files": "^2.13.1",
    "npm-check-updates": "^2.14.1",
    "wiredep": "^4.0.0"
  }
}
